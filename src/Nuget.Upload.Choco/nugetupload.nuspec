<?xml version="1.0" encoding="utf-8"?>
<!-- 
  For more info on this file format visit http://docs.nuget.org/docs/reference/nuspec-reference.
-->
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">  
  <metadata>
    <id>nugetupload</id>
    <version>$version$</version>
    <authors>Patrick Assuied</authors>
    <owners>Patrick Assuied</owners>
    <title>NuGet Upload tool</title>
    <summary>NuGet Upload tool</summary>
    <description>A command line tool allowing easy upload of a given Nuget's and dependencies' dlls to current location and subfolders.</description>
    <projectUrl>https://github.com/passuied/nuget-upload</projectUrl>
    <packageSourceUrl>https://github.com/passuied/nuget-upload</packageSourceUrl>
    <tags>nuget upload dll monolith admin</tags>
    <releaseNotes>
      Documentation and release notes can be found at https://github.com/passuied/nuget-upload
    </releaseNotes>
  <dependencies>
    <dependency id="nuget.commandline" version="4.9.3" />
  </dependencies>
  </metadata>
  <files>
    <file src="tools\chocolateyInstall.ps1" target="tools"/>
    <file src="tools\chocolateyUninstall.ps1" target="tools"/>
    <file src="tools\license.txt" target="tools"/>
	  <file src="..\..\artifact\NuGetUploadSetup.msi" target="tools"/>
  </files>
</package>